{
    "version": 2,
    "builds": [
        {
            "src": "src/app.js",
            "use": "@vercel/node@latest"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app.js"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "BASE_URL": "https://movie-gilt-six.vercel.app/"
    }
} 